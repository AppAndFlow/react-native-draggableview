{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": false,
    "target": "es5",
    "types": ["node"],
    "outDir": "compiled",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "skipLibCheck": true
  }
}
